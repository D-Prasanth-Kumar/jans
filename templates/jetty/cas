JAVA_HOME=%(jre_home)s
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS="-server -Xms128m -Xmx%(cas_max_heap_mem)sm -XX:MaxMetaspaceSize=%(cas_max_meta_mem)sm -XX:+DisableExplicitGC -Dgluu.base=%(gluuBaseFolder)s -Dserver.base=%(jetty_base)s/cas"

JETTY_HOME=%(jetty_home)s
JETTY_BASE=%(jetty_base)s/cas
JETTY_USER=jetty
JETTY_ARGS="jetty.http.host=localhost jetty.http.port=8083"
TMPDIR=%(jetty_dist)s/temp
