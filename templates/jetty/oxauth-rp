JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS="-server -Xms256m -Xmx1024m -XX:+DisableExplicitGC -Dgluu.base=%(gluuBaseFolder)s -Dcatalina.base=%(jetty_base)s/oxauth-rp"

