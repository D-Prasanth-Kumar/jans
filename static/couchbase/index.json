{
  "!bucket_prefix!_user": {
    "attributes": [
      ["objectClass"], 
      ["creationDate"], 
      ["inum"], 
      ["del","exp"], 
      ["uid"], 
      ["mail"], 
      ["jansStatus"], 
      ["jansApplication"], 
      ["jansDeviceHashCode"], 
      ["jansDeviceKeyHandle"],
      ["jansAuthClientId"],
      ["jansAuthUserId"]
    ],
    "static" : [
      [["LOWER(`uid`)"], "objectClass = \"jansPerson\""], 
      [["LOWER(`mail`)"], "objectClass = \"jansPerson\""], 
      [["exp"], "del=true"] 
    ]
  },

  "!bucket_prefix!": {
    "attributes": [
      ["objectClass"],
      ["jansScriptType"], 
      ["iat"], 
      ["jansAuthClientSecretExpiresAt"], 
      ["creationDate"], 
      ["tknCde"], 
      ["inum"], 
      ["displayName"], 
      ["jansAuthRegistrationAccessToken"], 
      ["jansAuthUmaScope"], 
      ["mail"], 
      ["jansId"], 
      ["jansTicket"], 
      ["del"],
      ["ssnId"],
      ["jansAuthClientId"],
      ["clnId"],
      ["jansAuthUserDN"],
      ["jansState"],
      ["del","exp"],
      ["objectClass","creationDate","jansStatus"]
    ],
    "static" : [
      [["creationDate"], "objectClass = \"jansPasswordResetRequest\""],
      [["jansScriptType"], "objectClass = \"jansCustomScript\""],
      [["jansApplicationType", "jansMetricType", "jansStartDate", "jansEndDate"], "objectClass = \"jansMetric\""],
      [["exp"], "del=true"],
      [["jansStatus", "exp"], "objectClass = \"cibaRequest\""]
        
    ]
  },

  "!bucket_prefix!_site": {
    "attributes": [
        ["objectClass"], 
        ["inum"]
    ], 
    "static" : [
    ]
  },

  "!bucket_prefix!_statistic": {
    "attributes": [
      ["objectClass"], 
      ["jansStartDate"], 
      ["del"], 
      ["exp"]
    ],
    "static" : [
    ]
  },

  "!bucket_prefix!_cache": {
    "attributes":[
    ],
    "static" : [
    ]
  },

  "!bucket_prefix!_token": {
    "attributes": [
      ["authzCode"]
    ],
    "static" : [
        [["grtId"], "objectClass = \"jansToken\""],
        [["ssnId"], "objectClass = \"jansToken\""]
    ]
  },

  "!bucket_prefix!_session": {
    "attributes": [
      ["jansUsrDN"]
    ],
    "static" : [
        [["sid"], "objectClass = \"jansToken\""],
        [["exp"], "objectClass = \"jansToken\""]
    ]
  },

  "__common__": {
    "attributes":[
    ],
    "static":[
    ]
  }

}
